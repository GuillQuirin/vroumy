<!DOCTYPE html>
<html>
  	<head>
	    <meta charset="utf-8">
	  	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	  	<meta name="viewport" content="width=device-width, initial-scale=1">
	    <title>Vroumy</title>
	    <%= csrf_meta_tags %>

	    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
	    <%= stylesheet_link_tag    'agency', media: 'all', 'data-turbolinks-track': 'reload' %>
	    <%= stylesheet_link_tag    'font-awesome.min', media: 'all', 'data-turbolinks-track': 'reload' %>
	    <%= javascript_include_tag 'jquery.min', 'data-turbolinks-track': 'reload' %>
	    <%= javascript_include_tag 'bootstrap.min', 'data-turbolinks-track': 'reload' %>
	    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js" integrity="sha384-mE6eXfrb8jxl0rzJDBRanYqgBxtJ6Unn4/1F7q4xRRyIw7Vdg9jP4ycT7x1iVsgb" crossorigin="anonymous"></script>
	    <%= javascript_include_tag 'jqBootstrapValidation', 'data-turbolinks-track': 'reload' %>
	    <%= javascript_include_tag 'contact_me', 'data-turbolinks-track': 'reload' %>
	    <script type="text/javascript" src="https://cdn.datatables.net/1.10.15/js/jquery.dataTables.min.js"></script>
	    <%= javascript_include_tag 'agency', 'data-turbolinks-track': 'reload' %>

	    <link href='https://fonts.googleapis.com/css?family=Kaushan+Script' rel='stylesheet' type='text/css'>
    	<link href='https://fonts.googleapis.com/css?family=Droid+Serif:400,700,400italic,700italic' rel='stylesheet' type='text/css'>
    	<link href='https://fonts.googleapis.com/css?family=Roboto+Slab:400,100,300,700' rel='stylesheet' type='text/css'>

    	<link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.15/css/jquery.dataTables.min.css"/>
  	</head>

    <br>
  	<body id="page-top" class="index">
	  	<!--<div class="container">
	  		<nav class="navbar navbar-inverse">
			  <div class="container-fluid">
			    <div class="navbar-header">
			      <a class="navbar-brand" class="text-uppercase" href="#">Vroumy</a>
			    </div>
			    <ul class="nav navbar-nav">
			      <li class="active"><a href="#">Accueil</a></li>
			      <li><%= link_to 'Liste des voitures', voitures_path %></li>
			      <li><%= link_to 'Liste des utilisateurs', utilisateurs_path %></li>
			      <li><%= link_to 'Liste des constructeurs', constructeurs_path %></li>
			      <li><%= link_to 'Liste des évènements', events_path %></li>
			    </ul>
			  </div>
			</nav
    	</div>-->

    	<nav id="mainNav" class="navbar navbar-default navbar-custom navbar-fixed-top">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header page-scroll">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span> Menu <i class="fa fa-bars"></i>
                </button>
                <%= link_to 'VROUMY', events_path, class:"navbar-brand page-scroll" %>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav navbar-right">
                    <li class="hidden">
                        <a href="#page-top"></a>
                    </li>
                    <li>
                        <%= link_to 'Notre concept', events_path + "#concept", class:"page-scroll" %>
                    </li>
                    <li>
                        <%= link_to 'Prochains évènements', events_path + "#events", class:"page-scroll" %>
                    </li>
                    <% if utilisateur_signed_in? %>
                    <li class="dropdown">
				        <a class="dropdown-toggle" data-toggle="dropdown" href="#"><%= current_utilisateur.email %>
				        <span class="caret"></span></a>
				        <ul class="dropdown-menu" id="drop_user">
				          <li><%= link_to 'Mon compte', utilisateur_url(current_utilisateur.id) %></li>
				          <li><%=button_to('Se déconnecter', destroy_utilisateur_session_path, :method => :delete) %></li>
				          <li><a href="#">Administration</a></li>
				        </ul>
				    </li>
			    	<li>
		    		
		    		</li>
		    		<% else %>
		    		<li>
		      		<%= link_to 'Se connecter', new_utilisateur_session_path %>
		    		<% end %></li>
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container-fluid -->
    </nav>
    	  		<% flash.each do |name, msg| %>
	      	<div class="alert alert-<%= notice_name(name) %>">
	        	<a class="close" data-dismiss="alert">x</a>
	        <%= content_tag :div, msg, id: "flash_#{name}" %>
	      	</div>
	    	<% end %>
    <%= yield %>
  	</body>
</html>
