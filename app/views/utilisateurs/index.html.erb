<p id="notice"><%= notice %></p>

<h1>Utilisateurs</h1>

<table>
  <thead>
    <tr>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Email</th>
      <th>Numaddress</th>
      <th>Address</th>
      <th>City</th>
      <th>Phone</th>
      <th>Isactive</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @utilisateurs.each do |utilisateur| %>
      <tr>
        <td><%= utilisateur.firstName %></td>
        <td><%= utilisateur.lastName %></td>
        <td><%= utilisateur.email %></td>
        <td><%= utilisateur.numAddress %></td>
        <td><%= utilisateur.address %></td>
        <td><%= utilisateur.city %></td>
        <td><%= utilisateur.phone %></td>
        <td><%= utilisateur.isActive %></td>
        <td><%= link_to 'Show', utilisateur %></td>
        <td><%= link_to 'Edit', edit_utilisateur_path(utilisateur) %></td>
        <td><%= link_to 'Destroy', utilisateur, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Utilisateur', new_utilisateur_path %>
